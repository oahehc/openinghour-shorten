{
  "name": "openinghour-shorten",
  "version": "1.0.0",
  "description": "shorten google map opening hour data",
  "main": "index.js",
  "repository": "git@github.com:oahehc/openinghour-shorten.git",
  "author": "oahehc <oahehc@gmail.com>",
  "license": "MIT",
  "keywords": [
    "google_map",
    "opening_hours"
  ],
  "scripts": {
    "test": "jest --coverage",
    "test:watch": "jest --watchAll"
  },
  "jest": {
    "collectCoverageFrom": [
      "*.{js,jsx}"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 99,
        "branches": 99,
        "functions": 99,
        "lines": 99
      }
    },
    "moduleDirectories": [
      "node_modules"
    ],
    "testRegex": "./.*\\.test\\.js$"
  },
  "devDependencies": {
    "jest": "^23.1.0"
  },
  "dependencies": {
    "immutable": "^3.8.2"
  }
}
